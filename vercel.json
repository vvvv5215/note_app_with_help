{
    "version": 2,
    "builds": [
        }
            "src": "api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/app.py"
        },
        {
            "src": "(.*)",
            "dest": "/api/app.py"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/app.py"
        }
    ]
}
